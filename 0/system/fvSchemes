FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         leastSquares;
    grad(T)         Gauss linear;
}

divSchemes
{
    default         none;
    div(rhoFlux,rho) Gauss upwind;
    div(j) Gauss linear;
}

laplacianSchemes
{
    default         none;
    laplacian(phi)  Gauss linear corrected;
    laplacian(T)  Gauss linear corrected;
    laplacian(A)  Gauss linear corrected;
    laplacian(sigma,phi)  Gauss linear corrected;
    laplacian(DT,T) Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
